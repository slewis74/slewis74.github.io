---
<?xml version="1.0" encoding="utf-8"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
  <channel>
    <title>slewis.me</title>
    <description>.NET dev going mobile</description>
    <link>http://www.slewis.me/</link>
    <pubDate>{{ site.time | date: "%a, %d %b %Y 00:00:00 GMT" }}</pubDate>
    <copyright>Copyright (c) 2014 Shannon Lewis</copyright>
    <atom:link href="http://www.slewis.me/rss.xml" rel="self" type="application/rss+xml" />
    {% for post in site.posts %}
    {% unless post.draft %}
    <item>
      <title>{{ post.title | xml_escape }}</title>
      <description>{{ post.content | xml_escape }}</description>
      <link>http://www.slewis.me{{ post.url }}/</link>
      <guid isPermaLink="true">http://www.slewis.me{{ post.url }}/</guid>
      <pubDate>{{ post.date | date: "%a, %d %b %Y 00:00:00 GMT" }}</pubDate>
    </item>
    {% endunless %}
    {% endfor %}
  </channel>
</rss>